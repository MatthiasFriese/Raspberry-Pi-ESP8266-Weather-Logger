html
  head
    title='Temperature Diagram'
    script(src='scripts/vis.js')
    link(href='scripts/vis.css', rel='stylesheet' type='text/css')
    style.
      body, html {
        font-family: sans-serif;
      }
  body
    div#visualization
    div#visualization2
    script.
      var container = document.getElementById('visualization');
      var items = !{data}
      items.map(function(e){e.x=new Date(e.x); return e;})


      var groups = new vis.DataSet();
      groups.add({
        id: 'humid',
        content: "Humidity",
        options: {
          yAxisOrientation: 'right',
          drawPoints: {
            style: 'square'
          }
      }});
      groups.add({
        id: 'temp',
        content: "Temperature",
        options: {
          yAxisOrientation: 'left',
          drawPoints: {
            style: 'square'
          },
          shaded: {
            orientation: 'bottom' // top, bottom
          }
      }});

      var dataset = new vis.DataSet(items);

      var options = {
        legend: {enabled: true},
      };
      var graph2d = new vis.Graph2d(container, dataset, groups, options);

      var container2 = document.getElementById('visualization2');
      var items2 = !{data2}
      items2.map(function(e){e.x=new Date(e.x); return e;})

      var groups2 = new vis.DataSet();
      groups2.add({
        id: 'preasure',
        content: "preasure",
        options: {
          yAxisOrientation: 'left',
          drawPoints: {
            style: 'square'
          },
          shaded: {
            orientation: 'bottom' // top, bottom
          }
      }});

      var dataset2 = new vis.DataSet(items2);

      var options2 = {
        legend: {enabled: true},
      };
      var graph2d = new vis.Graph2d(container2, dataset2, groups2, options2);
